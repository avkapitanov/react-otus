{
  "name": "react-otus",
  "version": "1.0.0",
  "description": "Game of Life",
  "main": "index.ts",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "jest-css-modules": "^2.1.0",
    "loki": "^0.28.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "scripts": {
    "test": "jest",
    "serve": "webpack serve --entry ./src/index.tsx",
    "build": "webpack --mode production",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avkapitanov/react-otus.git"
  },
  "author": "Artem Kapitanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avkapitanov/react-otus/issues"
  },
  "homepage": "https://github.com/avkapitanov/react-otus#readme",
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
